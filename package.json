{
  "devDependencies": {
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.19",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "esbuild-register": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts --fix && tsc",
    "bundle": "esbuild --bundle --platform=node --outdir=dist src/index_node.ts",
    "build": "npm run clean && npm run lint && npm run bundle",
    "start": "node --no-warnings --experimental-vm-modules -r esbuild-register src/index_node.ts"
  },
  "dependencies": {
    "cac": "^6.7.3",
    "clipboardy": "^2.3.0",
    "esbuild": "^0.12.19",
    "formdata-node": "^3.6.3",
    "http-proxy": "^1.18.1",
    "ink": "^3.0.9",
    "miniflare": "^1.3.3",
    "node-fetch": "github:tekwiz/node-fetch#fix/redirect-with-empty-chunked-transfer-encoding",
    "open": "^8.2.1",
    "react": "^16.14.0",
    "tmp-promise": "^3.0.2",
    "ws": "^8.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "eslint-plugin-react",
      "eslint-plugin-react-hooks"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/recommended",
          "plugin:react/recommended",
          "plugin:react-hooks/recommended"
        ],
        "rules": {
          "@typescript-eslint/consistent-type-imports": [
            "error"
          ],
          "no-empty-function": "off",
          "@typescript-eslint/no-empty-function": "off"
        }
      }
    ]
  },
  "cloudflare": {
    "//": "This is temporary, not the config format we actually want.",
    "account": "1fc1df98cc4420fe00367c3ab68c1639",
    "zone": null
  }
}
