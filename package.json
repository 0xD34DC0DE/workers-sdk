{
  "devDependencies": {
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "esbuild-register": "^3.0.0",
    "eslint": "^7.32.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts --fix && tsc",
    "bundle": "esbuild --bundle --platform=node --outdir=dist src/index_node.ts",
    "build": "npm run clean && npm run lint && npm run bundle",
    "start": "node -r esbuild-register src/index_node.ts"
  },
  "dependencies": {
    "cac": "^6.7.3",
    "esbuild": "^0.12.19",
    "formdata-node": "^3.6.3",
    "http-proxy": "^1.18.1",
    "node-fetch": "github:tekwiz/node-fetch#fix/redirect-with-empty-chunked-transfer-encoding",
    "tmp-promise": "^3.0.2",
    "ws": "^8.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
          "@typescript-eslint/consistent-type-imports": [
            "error"
          ]
        }
      }
    ]
  }
}
