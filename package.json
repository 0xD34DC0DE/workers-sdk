{
  "name": "rangla",
  "version": "0.0.0",
  "author": "wrangler@cloudflare.com",
  "description": "Command-line interface for all things Cloudflare Workers",
  "bin": "./bin/wrangler.js",
  "license": "MIT OR Apache-2.0",
  "bugs": {
    "url": "https://github.com/cloudflare/wrangler/issues"
  },
  "homepage": "https://github.com/cloudflare/wrangler#readme",
  "keywords": [
    "wrangler",
    "cloudflare",
    "workers",
    "cloudflare workers",
    "edge",
    "compute",
    "serverless",
    "serverless application",
    "serverless module",
    "wasm",
    "web",
    "assembly",
    "webassembly",
    "rust",
    "emscripten",
    "rust",
    "typescript",
    "graphql",
    "router",
    "http",
    "cli"
  ],
  "dependencies": {
    "esbuild": "^0.12.19",
    "miniflare": "^1.3.3",
    "yargs": "^17.1.1"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.2"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@types/cloudflare": "^2.7.6",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.19",
    "@types/ws": "^7.4.7",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "clipboardy": "^2.3.0",
    "cloudflare": "^2.9.1",
    "esbuild-register": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "find-up": "^6.0.0",
    "formdata-node": "^3.6.3",
    "http-proxy": "^1.18.1",
    "ink": "^3.2.0",
    "ink-table": "^3.0.0",
    "node-fetch": "github:tekwiz/node-fetch#fix/redirect-with-empty-chunked-transfer-encoding",
    "open": "^8.2.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "source-map-explorer": "^2.5.2",
    "tmp-promise": "^3.0.2",
    "typescript": "^4.3.5",
    "ws": "^8.2.0"
  },
  "files": [
    "bin",
    "miniflare-config-stubs",
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src/**",
    "prettify": "prettier src/** --write",
    "bundle": "node -r esbuild-register scripts/bundle.ts",
    "build": "npm run clean && npm run lint && npm run bundle",
    "start": "node --no-warnings --experimental-vm-modules -r esbuild-register src/index_node.ts"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "eslint-plugin-react",
      "eslint-plugin-react-hooks"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/recommended",
          "plugin:react/recommended",
          "plugin:react-hooks/recommended"
        ],
        "rules": {
          "@typescript-eslint/consistent-type-imports": [
            "error"
          ],
          "no-empty-function": "off",
          "@typescript-eslint/no-empty-function": "off"
        }
      }
    ]
  }
}
