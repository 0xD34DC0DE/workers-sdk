{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist/",
    "isolatedModules": true,
    "noEmit": true,
    "lib": ["esnext"]
  },
  "lib": ["esnext"],
  "exclude": ["node_modules/"]
}
